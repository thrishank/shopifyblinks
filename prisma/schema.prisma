generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 Int      @id @default(autoincrement())
  email              String   @unique
  password           String
  shopifyAccessToken String?
  shopifyWebsite     String?
  walletAddress      String?
  currency           String?
  createdAt          DateTime @default(now())
}
