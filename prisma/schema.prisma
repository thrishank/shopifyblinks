generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blink {
  id           String   @id @default(uuid())
  title        String
  description  String?
  image        String?
  price        String
  email        String[]
  walletAddres String
  createdAt    DateTime @default(now())
}
